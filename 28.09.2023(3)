#include <iostream>

using namespace std;

int main()
{
    int arr[ ] = {500, 4, 24, 15, 100, 105};
    int a;
    cout << "введи-на число: " << endl;
    cin >> a;
    int count = 0;
    for(int i = 0; i < 6; i++)
    {
    if(arr[i] % a == 0){
    count++;        
    }
    }
    cout << count;
    return 0;

}
