#include <iostream> 
using namespace std;

 
int main() { 
    char stroka[] = "Aaa1323#aa"; 
    char target = 'A';
    int count = 0;  
 
    for (int i = 0; i < sizeof(stroka); i++) { 
        if (stroka[i] == target) { 
            count++; 
        } 
    } 
 
cout << "сколько а: " << count << endl; 
 
    return 0; 
}
